generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Integration {
  id        String   @id @default(cuid())
  name      String
  provider  String
  apiKey    String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
