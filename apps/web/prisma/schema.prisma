generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Integration {
  id        String   @id @default(cuid())
  name      String
  provider  String   @unique
  apiKey    String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  active    Boolean  @default(false)
  baseUrl   String?
  model     String?
  region    String?
}
