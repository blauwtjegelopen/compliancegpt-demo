{
  "name": "compliancegpt-demo",
  "private": true,
  "packageManager": "pnpm@10.17.1",
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier -w .",
    "prisma:generate": "pnpm --filter @apps/web prisma:generate",
    "prisma:push": "pnpm --filter @apps/web prisma:push",
    "prisma:migrate": "pnpm --filter @apps/web prisma:migrate"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.14",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.11.0",
    "postcss": "^8.5.6",
    "prettier": "^3.3.3",
    "turbo": "^2.1.0",
    "typescript": "^5.6.2"
  },
  "overrides": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@netlify/functions": "^5.0.0",
    "@auth/core": "^0.27.0",
    "next-auth": "^4.24.7",
    "@prisma/client": "^6.16.3",
    "prisma": "^6.16.3"
  }
}